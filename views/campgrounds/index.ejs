<% layout('layouts/boilerplate') %> 
<!-- 위 경로의 ejs파일을 불러오기 -->
<h1>All Campgrounds</h1>
<div>
  <a href="/campgrounds/new">Add New Campground</a>
</div>
<ul>
  <% for(let campground of campgrounds) { %>
    <li>
      <a href="/campgrounds/<%=campground._id%>">
        <%= campground.title  %>
      </a>
    </li>
  <% } %> 
</ul>
